
<% if member.errors.full_messages.present? %>
  <% member.errors.full_messages.each do |error_message| %>
    <%= error_message %><br />
  <% end %>
<% end %>

<%= form_for(member) do |f| %>
  <%= f.label :name, I18n.t("activerecord.attributes.mameber.name_zh-TW") %>
  <%= f.text_field 'name_zh-TW' %> <br />
  <%= f.label :name, I18n.t("activerecord.attributes.mameber.name_en") %>
  <%= f.text_field 'name_en' %> <br />

  <%= f.submit %>
<% end %>
